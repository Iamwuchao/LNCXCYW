<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="Default" namespace="/" extends="json-default,struts-default ">
    <default-action-ref name="homepage"></default-action-ref>
      <action name="homepage">
        	<result>/jsp/homepage/homepage.jsp</result>
        </action> 
       
       <action name="test2" class="admin.NewsAction" method="newsList">
			<result name="success">
				/jsp/third/second_page.jsp
			</result>
		</action>
		
       <action name="newsAdd" class="admin.NewsAction" method="newsAdd">
			<result name="success">
				/jsp/super_admin/news_add.jsp
			</result>
		</action> 
              
       <action name = "newsSubmit" class="admin.NewsAction" method="newsSubmit">
       		<result name="success" type="json">
       			<param name="includeProperties">status</param>
       		</result>
       		<result name="error" type="json">
       			<param name="includeProperties">status</param>
       		</result>
       </action>   
       
       <action name="second_page" class="admin.NewsShowAction" method="showNews">
			<result name="success">
				/jsp/second_page/second.jsp
			</result>
		</action> 
		
		<action name="fd">
       		<result>/jsp/admin/news_manage.jsp</result>
       </action> 
       
       <action name="second">
       	<result>/jsp/second_page/second.jsp</result>
       </action>
       

       <action name="regist" class="homepage.AdminAction" method="regist">
        <result name="success" type="json">
        </result> 
       </action>

       
    </package>
    <package name="AdminDefault" extends="Default">	
	</package>
	<package name="HomepageDefault" extends="Default">
	</package>
    <!-- Add packages here -->
	 <include file="/action/test.xml"/>
	 <include file="/homepage/homepage.xml"/>
	<include file="/admin/admin.xml"/>
</struts>
